{
    "configurations": [
    {
        "name": "(gdb) Iniciar",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/simplevfs-test-unittest",
        "args": ["t 1"],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "Habilitar la impresión con sangría para gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            }
        ],
        "preLaunchTask": "dub Rebuild unittest"
    }
    ]

}